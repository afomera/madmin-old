<h1><%= @resource.class.name %> <%= @resource.id %></h1>

<table>
  <% @resource.attributes.each do |key, value| %>
    <tr>
      <td>
        <%= key %>
      </td>
      <td>
        <%= value %>
      </td>
    </tr>
  <% end %>
</table>

<%= link_to 'Edit', edit_resource_path(id: @resource.id) %>
<%= link_to 'Delete', resource_path(id: @resource.id), method: :delete %>
